package com.banking;

public class SBI extends Bank {
	String BankName;
	
	public SBI(float Principal, float TimePeriod, float SimpleInterest, float RateOfInterest, String BankName) 
	{ 
		super(Principal, TimePeriod, SimpleInterest, RateOfInterest);
		this.BankName = BankName;
	}
//	float res = (Principal*TimePeriod);
	RateOfInterest = 100*SimpleInterest/Principal*TimePeriod;
	System.out.println(RateOfInterest);

	}