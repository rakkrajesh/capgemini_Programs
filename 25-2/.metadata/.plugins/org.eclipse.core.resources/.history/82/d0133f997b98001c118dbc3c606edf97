package com.banking;

public class Bank {
	float Principal;
	int TimePeriod;
	float SimpleInterest;
	float RateOfInterest;
	
	public Bank(float principal, int timePeriod, float simpleInterest, float rateOfInterest) {
		Principal = principal;
		TimePeriod = timePeriod;
		SimpleInterest = simpleInterest;
		RateOfInterest = rateOfInterest;
		
		
	}
	
	
}
