package array;

public class SwappingIntArray {
	public static void main(String[] args) {
		int[] x = {10, 32, 34, 543, 53 };
		
		for(int i=0; i<x.length; i++) {
			System.out.println(x[i]);
		}
		System.out.println("+++++++++++++++++++++++++++++++++++++");
		
		for(int i=x.length-1; i>0; i--) {
			System.out.println(x[i]);
		}
	}
}
